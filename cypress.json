{
  "baseUrl": "http://localhost:4200",
  "blacklistHosts": ["*nr-data.net", "*.visualstudio.com"],
  "viewportWidth": 1225,
  "viewportHeight": 800,
  "env": {
    "apiUrlBase": "/sap/opu/odata/sap",
    "apiUrl": "/sap/opu/odata/sap/Z_CRM_ODATA_SRV",
    "apiUrlPaymentAgreement": "/sap/opu/odata/sap/Z_PAY_AGREEMENT_SERVICES_SRV_01"
  },
  "reporterOptions": {
    "mochaFile": "e2e-test-results/test-results-[hash].xml",
    "toConsole": true,
    "attachments": true,
    "testsuitesTitle": "Cypress Tests"
  },
  "watchForFileChanges": false,
  "fixturesFolder": "mocks/sap/",
  "video": false,
  "numTestsKeptInMemory": 1
}
